@model IEnumerable<DomingoRoofWorks.Models.Rates>

@{
    ViewData["Title"] = "Index";
}

<h1>List Of Rates</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RateID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RateID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RateID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
